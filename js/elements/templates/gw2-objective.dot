<link rel="stylesheet" type="text/css" href="css/gw2-objective.css" />
{{? context.type === "Castle"}}
	<link rel="stylesheet" type="text/css" href="css/obj-castle.css" />
{{?? context.type === "Keep"}}
	<link rel="stylesheet" type="text/css" href="css/obj-keep.css" />
{{?? context.type === "Tower"}}
	<link rel="stylesheet" type="text/css" href="css/obj-tower.css" />
{{?? context.type === "Camp"}}
	<link rel="stylesheet" type="text/css" href="css/obj-camp.css" />
{{?? context.type === "Ruins"}}
	<link rel="stylesheet" type="text/css" href="css/obj-ruins.css" />
{{?}}
<div class="objective {{=context.type}}" owner="Neutral">
	{{? context.type !== "Ruins"}}
	<div class="iteminfos">
		<div class="turnedtext"></div>
		<div class="marker"></div>
		<div class="info_wrapper">
			<div class="info">
				<div class="guild_emblem"></div>
				<span class="name">{{=context.name}}</span>
				<span class="timer"></span>
				<span class="guildname">
					<span class="guild_name"></span> [
					<span class="guild_tag"></span>]</span>
				<span class="claimed">-</span>
				<span class="dollies"></span>
				<span class="upgrades">
					<span class="upgrade"></span>
					<span class="upgrade"></span>
					<span class="upgrade"></span>
					<span class="upgrade"></span>
					<span class="upgrade"></span>
					<span class="upgrade"></span>
				</span>
			</div>
		</div>
	</div>
	{{?}}
</div>
