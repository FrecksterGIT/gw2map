<link rel="stylesheet" type="text/css" href="css/scoreboard.css" />
{{~context.matches :match:index}}
		<table data-match-id="{{=match.id}}">
			<thead>
			<tr class="header">
				<td class="worlds">World</td>
				<td class="income">Income</td>
				<td class="victory_points">Victory Points<canvas class="chart" width="64" height="64" style="width: 64px; height: 64px; transform: rotate(52.9157deg);"></canvas></td>
				<td class="current_score">Score</td>
				<td></td>
			</tr>
			</thead>
			<tbody>
			<tr class="green_world">
				<td class="name">{{=match.world_names.green}}<span>{{=match.all_world_names.green}}</span></td></td>
				<td class="income">{{=match.income.Green}}</td>
				<td class="victory_points">{{=match.victory_points.green}}</td>
				<td class="current_score">{{=match.currentScores.green}}</td>
				<td class="bar"><td style="width: 98.7879%;"></td></td>
			</tr>
			<tr class="blue_world">
				<td class="name">{{=match.world_names.blue}}<span>{{=match.all_world_names.blue}}</span></td>
				<td class="income">{{=match.income.Blue}}</td>
				<td class="victory_points">{{=match.victory_points.blue}}</td>
				<td class="current_score">{{=match.currentScores.blue}}</td>
				<td class="bar"><td style="width: 100%;"></td></td>
			</tr>
			<tr class="red_world">
				<td class="name">{{=match.world_names.red}}<span>{{=match.all_world_names.red}}</span></td></td>
				<td class="income">{{=match.income.Red}}</td>
				<td class="victory_points">{{=match.victory_points.red}}</td>
				<td class="current_score">{{=match.currentScores.red}}</td>
				<td class="bar"><td style="width: 73.9394%;"></td></td>
			</tr>
			</tbody>
		</div>
	</div>
{{~}}
