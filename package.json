		"chart.js": "^2.7.2",
		"sprintf-js": "^1.1.1",
		"universal-cookie": "^2.1.2"
	},
	"description": "Small Webapplication for World vs World in Guild Wars 2.",
	"scripts": {
		"prebuild": "npm-run-all clean cache vendor:*",
		"build": "npm-run-all webpack",
		"postbuild": "cpx ./docroot/** ./dist",
		"clean": "rimraf ./dist ./docroot/js/** ./src/static-cache",
		"prestart": "cross-env NODE_ENV=develop npm-run-all cache vendor:*",
		"start": "cross-env NODE_ENV=development npm-run-all -p webpack-watch karma-watch run-server",
		"test": "npm-run-all test:*",
		"test:stylelint": "stylelint docroot/css/*.css",
		"test:eslint": "eslint src/**/*.js",
		"test:karma": "cross-env NODE_ENV=test karma start",
		"precache": "rimraf ./src/static-cache && babel ./npm-scripts/ --out-dir ./.tmp --copy-files --quiet",
		"cache": "node ./.tmp/cache.js",
		"postcache": "rimraf ./.tmp",
		"vendor:chartjs": "cpx ./node_modules/chart.js/dist/Chart.min.js ./docroot/js/vendor",
		"run-server": "simple-server docroot 8000",
		"karma-watch": "karma start --no-single-run --auto-watch",
		"webpack": "webpack --config ./.config/webpack.config.js --mode=production",
		"webpack-watch": "webpack --config ./.config/webpack.config.js --watch --mode=development"
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/FrecksterGIT/gw2map.git"
	},
	"keywords": [
		"gw2",
		"wvw"
	],
	"bugs": {
		"url": "https://github.com/FrecksterGIT/gw2map/issues"
	},
	"homepage": "https://github.com/FrecksterGIT/gw2map#readme"
}
