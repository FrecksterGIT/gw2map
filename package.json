  "devDependencies": {
    "babel-core": "^6.26.0",
    "babel-loader": "^7.1.2",
    "cpx": "^1.5.0",
    "dotjs-loader": "^0.1.0",
    "eslint": "^4.16.0",
    "eslint-loader": "^1.9.0",
    "npm-run-all": "^4.1.2",
    "rimraf": "^2.6.2",
    "simple-server": "^1.1.1",
    "stylelint": "^8.4.0",
    "stylelint-config-recommended": "^2.0.1",
    "stylelint-order": "^0.8.0",
    "webpack": "^3.10.0"
  },
  "dependencies": {
    "deep-diff": "^0.3.8",
    "dot": "^1.1.2",
    "universal-cookie": "^2.1.2"
  },
  "description": "Small Webapplication for World vs World in Guild Wars 2.",
  "scripts": {
    "build": "npm-run-all clean-dist webpack clean-dist copy",
    "clean-dist": "rimraf ./dist",
    "copy": "npm-run-all copy:*",
    "copy:assets": "cpx ./assets/** ./dist/assets",
    "copy:css": "cpx ./css/** ./dist/css",
    "copy:build": "cpx ./build/** ./dist/build",
    "copy:base": "cpx ./index.html ./dist",
    "copy:redirect": "cpx ./.htaccess ./dist",
    "develop": "npm-run-all -p webpack-watch run-server",
    "eslint": "eslint js/**/*.js",
    "run-server": "simple-server 8000",
    "stylelint": "stylelint css/*.css",
    "test": "npm-run-all stylelint eslint",
    "webpack": "webpack",
    "webpack-watch": "webpack --watch"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/FrecksterGIT/gw2map.git"
  },
  "keywords": [
    "gw2",
    "wvw"
